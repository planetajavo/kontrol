{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "index-docs.html",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist-docs"
      }
    }
  ],
  "routes": [
    {
      "src": "/docs/(.*)",
      "dest": "/index-docs.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
